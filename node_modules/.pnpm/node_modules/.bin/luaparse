#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Website build/E2-Gmod-random/node_modules/.pnpm/luaparse@0.2.1/node_modules/luaparse/bin/node_modules:/mnt/d/Website build/E2-Gmod-random/node_modules/.pnpm/luaparse@0.2.1/node_modules/luaparse/node_modules:/mnt/d/Website build/E2-Gmod-random/node_modules/.pnpm/luaparse@0.2.1/node_modules:/mnt/d/Website build/E2-Gmod-random/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Website build/E2-Gmod-random/node_modules/.pnpm/luaparse@0.2.1/node_modules/luaparse/bin/node_modules:/mnt/d/Website build/E2-Gmod-random/node_modules/.pnpm/luaparse@0.2.1/node_modules/luaparse/node_modules:/mnt/d/Website build/E2-Gmod-random/node_modules/.pnpm/luaparse@0.2.1/node_modules:/mnt/d/Website build/E2-Gmod-random/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../luaparse/bin/luaparse" "$@"
else
  exec node  "$basedir/../luaparse/bin/luaparse" "$@"
fi
